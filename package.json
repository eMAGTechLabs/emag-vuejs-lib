{
  "name": "emag-vuejs-lib",
  "version": "0.1.0",
  "description": "JS library for admin applications based on Vue JS and eMAG Apps UI KIT",
  "author": "Valentin Bica | eMAG, Software Development <valentin.bica@emag.ro>",
  "main": "dist/emag-vuejs-lib.js",
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:prod": "webpack --progress --hide-modules --config  build/webpack.config.production.js",
    "build:dev": "webpack --progress --hide-modules --config  build/webpack.js",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "precommit": "npm run unit"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "vue": "2.1.4",
    "vue-i18n": "^4.8.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.2.0",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "emag-apps-ui-kit": "https://github.com/eMAGTechLabs/emag-apps-ui-kit.git",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.0",
    "chai": "^3.5.0",
    "css-loader": "^0.21.0",
    "file-loader": "^0.10.1",
    "function-bind": "^1.0.2",
    "fs-extra": "^1.0.0",
    "gh-pages": "^0.11.0",
    "husky": "^0.13.1",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.0",
    "less-loader": "^2.2.3",
    "mocha": "^3.1.0",
    "node-sass": "^3.10.1",
    "phantomjs-prebuilt": "^2.1.3",
    "prismjs": "^1.5.1",
    "sass-loader": "^3.2.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "7.1.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.0",
    "webpack-merge": "^2.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      [
        "vueify"
      ]
    ]
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
